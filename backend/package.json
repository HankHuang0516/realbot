{
  "name": "realbot-backend",
  "version": "1.0.0",
  "description": "Backend server for Claw Live Wallpaper Android app",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest --testPathPattern=tests/jest",
    "test:stress": "node stress-test.js",
    "test:verbose": "VERBOSE=true node stress-test.js",
    "test:production": "API_BASE=https://eclaw.up.railway.app node stress-test.js",
    "test:persistence": "node test-persistence.js",
    "test:persistence:check": "node test-persistence.js --check",
    "lint": "eslint . --ext .js --ignore-path .eslintignore"
  },
  "keywords": [
    "realbot",
    "claw",
    "live-wallpaper",
    "api"
  ],
  "author": "Hank",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "flickr-sdk": "^6.3.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pg": "^8.18.0",
    "resend": "^6.9.2"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "eslint": "^9.39.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^7.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "commonjs"
}
